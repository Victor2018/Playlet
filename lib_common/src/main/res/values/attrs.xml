            <enum name="top_to_bottom" value="1"/>
            <enum name="right_to_left" value="2"/>
            <enum name="left_to_right" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="LightAnimationView">
        <attr name="light_direction" format="enum">
            <enum name="left_to_right" value="0"/>
            <enum name="right_to_left" value="1"/>
        </attr>
        <attr name="light_color" format="color"/>
        <attr name="light_duration" format="integer" />
        <attr name="light_peak_alpha" format="integer"/>
        <attr name="light_width" format="dimension"/>
    </declare-styleable>

</resources>